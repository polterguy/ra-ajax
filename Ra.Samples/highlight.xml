      <kw>catch</kw>
      <kw>char</kw>
      <kw>checked</kw>
      <kw>class</kw>
      <kw>const</kw>
      <kw>continue</kw>
      <kw>decimal</kw>
      <kw>default</kw>
      <kw>delegate</kw>
      <kw>do</kw>
      <kw>double</kw>
      <kw>else</kw>
      <kw>enum</kw>
      <kw>event</kw>
      <kw>explicit</kw>
      <kw>extern</kw>
      <kw>false</kw>
      <kw>finally</kw>
      <kw>fixed</kw>
      <kw>float</kw>
